#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules/react-scan/bin/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules/react-scan/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules/react-scan/bin/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules/react-scan/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.2_react-dom@19.1.0_react@19.1.0__react-router-dom@7._53d424dd83f18108c15d1d922ebd0f4f/node_modules:/home/kali/Desktop/Sfera-Academyy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scan/bin/cli.js" "$@"
else
  exec node  "$basedir/../react-scan/bin/cli.js" "$@"
fi
